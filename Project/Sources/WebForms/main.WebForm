				"doc": [
					{
						"type": "paragraph",
						"children": [
							{
								"text": "- Assetify is an all-in-one platform that enables you to track IT assets, manage software and licenses, and handle incidents effortlessly, ensuring seamless IT operations.",
								"italic": false,
								"underlined": false,
								"strikethrough": false
							}
						]
					},
